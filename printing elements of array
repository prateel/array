//taking user input for array
#include <stdio.h>

int main()
{
    int n[5],i;
    
    printf("input five elements in array:");
    
    for(i=0;i<5;i++){
        
        scanf("%d",&n[i]);
        
    }
    for(i=0;i<5;i++){
        printf("%d",n[i]);
    }
    

    return 0;
}



// alraedy defined array
#include <stdio.h>

int main()
{
    int i;
    int n[4]={2,56,76,454};
      
        
    for(i=0;i<4;i++){
        printf("%d\n ",n[i]);
        printf("%p\n ",n+i);
       
    }
         return 0;
}
