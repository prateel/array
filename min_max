#include <stdio.h>

int main()
{
    int a[100],n,i,min=0,max=0;
    printf("enter the size of the array:\n");
    scanf("%d",&n);
    printf("enter the elements of array;\n ");
    
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        if(min>a[i])
        min=a[i];
        if(max<a[i])
        max=a[i];
    }
    printf("minimum no is %d ",min);
    printf("maximum no is %d",max);

    return 0;
}
