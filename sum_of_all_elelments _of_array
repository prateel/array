#include <stdio.h>

int main()
{
    int a[10],n,i,sum=0;
    printf("enter the size of the array:\n");
    scanf("%d",&n);
    
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        sum=sum+a[i];
    }
    printf("sum of all elements of an array %d ",sum);

    return 0;
}
