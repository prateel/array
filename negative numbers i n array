#include <stdio.h>

int main()
{
    int n;
    printf("Enter the range\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter negative number\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]<0)
        printf("%d\t",a[i]);
    }
    return 0;
}
