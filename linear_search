#include <stdio.h>

int main()
{
    int a[10],item,i,n;
    printf("enter the value to be searched:");
    scanf("%d",&item);
    for(i=0;i<10;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<10;i++)
    {
        if(item==a[i])
        printf(" place value is: %d",i+1);
    }

     return 0;
}
